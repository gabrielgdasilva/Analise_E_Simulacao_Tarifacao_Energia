@model IEnumerable<Analise_E_Simulacao_Tarifacao_Energia.Models.FabricaModel>

@{
    ViewBag.Title = "Lista de Fabricas";
}

<h2>List</h2>

<p>
    @Html.ActionLink("Cadastrar Nova", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cnpj)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Endereco)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MascaraCNPJ)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Endereco)
        </td>
        <td>
            @Html.ActionLink("Ver contas", "List", "Conta", new { id=item.FabricaID }, null) |
            @Html.ActionLink("Editar", "Edit", new { id = item.FabricaID }) |
            @*@Html.ActionLink("Detalhes", "Details", new { id = item.FabricaID }) |*@
            @Html.ActionLink("Excluir", "Delete", new { id = item.FabricaID })
        </td>
    </tr>
}

</table>
